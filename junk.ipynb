    "# ChatGPT-generated\n",
    "function blockdiag1(M1::Matrix, M2::Matrix)\n",
    "    m1, n1 = size(M1)\n",
    "    m2, n2 = size(M2)\n",
    "    T = promote_type(eltype(M1), eltype(M2)) # unify element type\n",
    "    B = zeros(T, m1 + m2, n1 + n2)\n",
    "    B[1:m1, 1:n1] = M1\n",
    "    B[m1+1:end, n1+1:end] = M2\n",
    "    return B\n",
    "end"
   ]
  }
